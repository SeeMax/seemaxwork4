<!DOCTYPE html>
<html>
  <head>
    <title>SeeMaxWork</title>
    <%= favicon_link_tag 'seemaxfav.png' %>
    <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
    <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
    <%= csrf_meta_tags %>
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-5190084-1', 'seemaxwork.com');
      ga('send', 'pageview');
    </script>
  </head>
  <nav>
    <div class="nav_item">
      <div class="dot red"></div>
      <a class="transition" href="/work">
        WORK
      </a>
    </div>
    <div class="nav_item">
      <div class="dot yellow"></div>
      <a class="transition" href="/logos">
        LOGOS
      </a>
    </div>
    <div class="nav_item">
      <div class="dot green"></div>
      <a class="transition" href="/fun">
        FUN
      </a>
    </div>
    <div class="nav_item">
      <div class="dot black"></div>
      <a class="transition" href="/email">
        EMAIL
      </a>
    </div>
  </nav>

  <%= yield %>

  <footer>
    <%= render 'layouts/footer' %>
  </footer>
  </body>
</html>
