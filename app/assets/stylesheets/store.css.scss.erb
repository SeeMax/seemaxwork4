@import "variables.css.scss" , "mixins.css.scss";

.store-page {
  color:$blue;
  background-color:$blue;
  height:100%;
  width:100%;
  margin-top:-95px;
  padding-top:95px;

  .close {
    background:$blue;
  }

  p {
    a {
      color:$red;
      border-bottom: 1px white solid;
      padding-bottom: 0px;
      @include transition;
    }

    a:hover {
      border-bottom: 1px $red solid;
    }
  }
}