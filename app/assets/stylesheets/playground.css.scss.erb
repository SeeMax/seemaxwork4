@import "variables.css.scss";


.playground-page {
  color:$grey;
  background-color:grey;
  min-height:100%;

  .button {
    position:relative;
    z-index:100;
    width:300px;
    margin:0px auto;
    padding:10px 20px;
    background:white;
    color:$grey;
    font-weight:400;
    font-size:40px;
    text-align: center;
    cursor:pointer;
  }

  .logo {
    width:100%;
    height:auto;
    max-height:600px;
  }

  svg {
    width: 100%;
    height: 100%;
  }
}

