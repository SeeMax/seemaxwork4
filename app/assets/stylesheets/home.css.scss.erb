@import "variables.css.scss";

.main {
min-width:100%;
min-height:100%;


.logo {
  text-align:center;
  padding-top: 160px;

  img {
    height:420px;
}

}

.bg {
  z-index:-1;
}

  img.bg {
    /* Set rules to fill background */
    min-height: 100%;
    min-width: 1024px;

    /* Set up proportionate scaling */
    width: 100%;
    height: auto;

    /* Set up positioning */
    position: fixed;
    top: 0;
    left: 0;
  }


}