@import "variables.css.scss";

.main {
min-width:100%;
min-height:100%;


.logo {
  margin: 0 auto;
  width:250px;
  padding-top:170px;
  text-align:center;
  font-size:40px;
  letter-spacing:6px;

  img {
    padding-top:20px;
  }
}

.bg {
  z-index:-1;
}

  img.bg {
    /* Set rules to fill background */
    min-height: 100%;
    min-width: 1024px;

    /* Set up proportionate scaling */
    width: 100%;
    height: auto;

    /* Set up positioning */
    position: fixed;
    bottom: 0;
    right: 0;
  }


}