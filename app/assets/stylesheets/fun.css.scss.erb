@import "variables.css.scss" , "mixins.css.scss";

.fun-page {
  color:$green;
  background-color:$green;

  .close {
      background:$green;
    }

  .credits {
    color:$green;
  }

  p {
    a {
      color:$green;
      border-bottom: 1px white solid;
      padding-bottom: 0px;
      @include transition;
    }

    a:hover {
      border-bottom: 1px $green solid;
    }
  }
}