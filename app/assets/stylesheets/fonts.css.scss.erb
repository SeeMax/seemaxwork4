@font-face {
    font-family: 'Mensch';
    src: url('<%= asset_path 'mensch.eot?#iefix' %>') format('embedded-opentype'),
         url('<%= asset_path 'mensch.woff' %>') format('woff'),
         url('<%= asset_path 'mensch.ttf' %>') format('truetype'),
         url('<%= asset_path 'mensch.svg#svgFontName' %>') format('svg');
         font-weight: 400;
         font-style: normal;
}


