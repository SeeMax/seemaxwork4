.internal-oddball {
  opacity:0;

  .text-headline {
  margin: 0;
  padding: 0;
  text-align: left;
  font-family: "averta";
  font-weight: 500;
  font-size: 65px;
  line-height: 1em;
  fill: #00A187; }

  .subhead {
    margin: 0;
    padding: 0;
    text-align: center;
    font-family: "averta";
    font-weight: 100;
    font-size: 40px;
    line-height: 1em;
    fill: #00A187; }

  .hidden {
    visibility: hidden; }

  .stroked {
    fill: white;
    stroke: #00A187;
    stroke-width: 8;
    stroke-miterlimit: 10;
    outline: 1px solid transparent; }

  .outlined {
    fill: none;
    stroke: #00A187;
    stroke-width: 8;
    stroke-miterlimit: 10;
    outline: 1px solid transparent; }

  .filled {
    fill: #00A187;
    stroke: none;
    outline: 1px solid transparent; }

  .down-arrow {
    fill: none;
    stroke: #FAF39F;
    stroke-width: 4;
    stroke-miterlimit: 10;
    outline: 1px solid transparent; }
}